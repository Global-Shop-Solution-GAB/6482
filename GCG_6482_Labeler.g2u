Program.Sub.cmdLblBrowse_Click.Start
f.Intrinsic.Control.Try
V.Local.sError.Declare

v.Local.sTitle.Declare
v.Local.iWidth.Declare
v.Local.sSQL.Declare
v.Local.sRet.Declare
v.Local.sJob.Declare
v.Local.fQty.Declare

f.Intrinsic.Control.SelectCase(v.Screen.LabelReprint!ddlReprint.Text)

f.Intrinsic.Control.Case("Reprint Individual Serial Number")

	f.Intrinsic.String.Split("SERIAL*!*JOB*!*PART*!*DESCRIPTION","*!*",v.Local.sTitle)
	f.Intrinsic.String.Split("1200*!*800*!*800*!*1200","*!*",v.Local.iWidth)
	
	f.ODBC.Connection!conx.OpenCompanyConnection
	
	v.Local.sSQL.Set("select SERIAL_NUMBER, JOB, PART, MODEL from GCG_6482_Label_Serial")
	F.Intrinsic.UI.Browser("Label Serial Number Browser","conx",v.Local.sSQL,v.Local.sTitle,v.Local.iWidth,v.Local.sRet)
	
	f.ODBC.Connection!conx.Close
	
	f.Intrinsic.Control.If(v.Local.sRet,<>,"***CANCEL***")
		
		f.Intrinsic.String.Split(v.Local.sRet,"*!*",v.Local.sRet)
		f.Data.DataTable.AddRow("dtReprint","LABEL",v.Local.sRet(0))
	
	f.Intrinsic.Control.EndIf
	
f.Intrinsic.Control.Case("Reprint All Items in Work Order")

	f.Intrinsic.UI.Browser(1000,"",v.Local.sRet)

	f.Intrinsic.Control.If(v.Local.sRet,<>,"***CANCEL***")
		f.Intrinsic.String.Split(v.Local.sRet,"*!*",v.Local.sRet)
		f.Intrinsic.Control.If(v.Local.sRet(1),<>,"   ")
			f.Intrinsic.String.Build("{0}-{1}",v.Local.sRet(0),v.Local.sRet(1),v.Local.sJob)
		f.Intrinsic.Control.Else
			v.Local.sJob.Set(v.Local.sRet(0))
		f.Intrinsic.Control.EndIf
		f.ODBC.Connection!conx.OpenCompanyConnection
		f.Intrinsic.String.Build("select QTY_ORDER from V_JOB_HEADER where JOB = '{0}'",v.Local.sJob,v.Local.sSQL)
		f.ODBC.Connection!conx.ExecuteandReturn(v.Local.sSQL,v.Local.fQty)
		f.ODBC.Connection!conx.Close
		f.Data.DataTable.AddRow("dtReprint","LABEL",v.Local.sJob,"QTY_PART",v.Local.fQty,"QTY_LABEL",v.Local.fQty,"UL",False,"BOX",False)
	f.Intrinsic.Control.EndIf
	
